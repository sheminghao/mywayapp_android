                            android:layout_width="40px"
                            android:layout_height="40px"
                            android:layout_gravity="center"
                            android:visibility="gone"
                            app:SpinKit_Color="@color/white" />

                    </com.zhy.autolayout.AutoFrameLayout>

                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:id="@+id/layout_finger_mark"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:background="#32000000"
                    android:orientation="horizontal"
                    android:padding="20px">

                    <ImageView
                        android:layout_width="36px"
                        android:layout_height="36px"
                        android:src="@mipmap/guzhangjiance_icon_zhiwenmokuaitongxinguzhang" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20px"
                        android:layout_weight="1"
                        android:text="@string/fault_of_fingerprint_module"
                        android:textColor="@color/white"
                        android:textSize="36px" />

                    <com.zhy.autolayout.AutoFrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/img_right_8"
                            android:layout_width="40px"
                            android:layout_height="40px"
                            android:src="@mipmap/guzhangjiance_right"
                            android:visibility="gone" />

                        <com.github.ybq.android.spinkit.SpinKitView
                            android:id="@+id/empty_loading_8"
                            style="@style/SpinKitView.Circle"
                            android:layout_width="40px"
                            android:layout_height="40px"
                            android:layout_gravity="center"
                            android:visibility="gone"
                            app:SpinKit_Color="@color/white" />

                    </com.zhy.autolayout.AutoFrameLayout>

                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:id="@+id/layout_gps"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:padding="20px">

                    <ImageView
                        android:layout_width="36px"
                        android:layout_height="36px"
                        android:src="@mipmap/guzhangjiance_gps_tongxinguzhang" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20px"
                        android:layout_weight="1"
                        android:text="@string/fault_of_GPS"
                        android:textColor="@color/white"
                        android:textSize="36px" />

                    <com.zhy.autolayout.AutoFrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/img_right_9"
                            android:layout_width="40px"
                            android:layout_height="40px"
                            android:src="@mipmap/guzhangjiance_right"
                            android:visibility="gone" />

                        <com.github.ybq.android.spinkit.SpinKitView
                            android:id="@+id/empty_loading_9"
                            style="@style/SpinKitView.Circle"
                            android:layout_width="40px"
                            android:layout_height="40px"
                            android:layout_gravity="center"
                            android:visibility="gone"
                            app:SpinKit_Color="@color/white" />

                    </com.zhy.autolayout.AutoFrameLayout>

                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:id="@+id/layout_gprs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#32000000"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:padding="20px">

                    <ImageView
                        android:layout_width="36px"
                        android:layout_height="36px"
                        android:src="@mipmap/guzhangjiance_gpstongxinguzhang" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20px"
                        android:layout_weight="1"
                        android:text="@string/fault_of_GPRS"
                        android:textColor="@color/white"
                        android:textSize="36px" />

                    <com.zhy.autolayout.AutoFrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/img_right_10"
                            android:layout_width="40px"
                            android:layout_height="40px"
                            android:src="@mipmap/guzhangjiance_right"
                            android:visibility="gone" />

