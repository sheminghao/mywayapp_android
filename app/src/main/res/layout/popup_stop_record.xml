<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="30px"
    android:gravity="center_horizontal"
    android:paddingBottom="150px" >

    <TextView
        android:id="@+id/tv_confirm"
        android:layout_width="616px"
        android:layout_height="152px"
        android:text="@string/confirm_to_end"
        android:textSize="36px"
        android:textColor="#333333"
        android:gravity="center"
        android:background="@mipmap/jieshujiluguiji_btn_queding"/>

    <TextView
        android:id="@+id/tv_cancel"
        android:layout_width="616px"
        android:layout_height="152px"
        android:text="@string/dialog_cancel"
        android:textSize="36px"
        android:layout_marginTop="-20px"
        android:textColor="@color/white"
        android:gravity="center"
        android:background="@mipmap/jieshujiluguiji_btn_quxiao"/>

</com.zhy.autolayout.AutoLinearLayout>
